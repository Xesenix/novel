{
	"extends": "./tsconfig.xes.json",
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"types": [
			"jasmine",
			"node"
		]
	},
	"files": [
		"main.ts",
		"polyfills.ts",
		"test.xes.ts"
	],
	"include": [
		"xes/*.spec.ts",
		"xes/*.d.ts",
		"app/*.spec.ts",
		"app/*.d.ts",
		"xes/**/*.spec.ts",
		"xes/**/*.d.ts",
		"app/**/*.spec.ts",
		"app/**/*.d.ts"
	],
	"exclude": []
}
